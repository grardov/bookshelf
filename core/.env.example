# Supabase Configuration
# Get these values from `supabase status` after running `supabase start` in the db/ directory
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Discogs OAuth 1.0a Configuration (optional, for collection sync)
# Get credentials from https://www.discogs.com/settings/developers
DISCOGS_CONSUMER_KEY=your-discogs-consumer-key
DISCOGS_CONSUMER_SECRET=your-discogs-consumer-secret
DISCOGS_USER_AGENT=Bookshelf/0.1.0
DISCOGS_PERSONA_ACCESS_TOKEN=your-discogs-persona-access-token

# State encryption key for OAuth flow
# Generate with: python -c "import secrets; print(secrets.token_hex(32))"
STATE_ENCRYPTION_KEY=your-64-character-hex-string
