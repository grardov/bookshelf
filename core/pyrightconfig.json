{
  "include": ["src"],
  "exclude": [
    "**/__pycache__",
    ".venv",
    "**/.pytest_cache"
  ],
  "extraPaths": ["src"],
  "pythonVersion": "3.12",
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "stubPath": "",
  "venvPath": ".",
  "venv": ".venv"
}
